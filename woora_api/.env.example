# Woora Building API - Environment Variables
# Copy this file to .env and fill in your actual values

# CRITICAL: Security Keys (REQUIRED - no fallback)
SECRET_KEY=your-super-secret-key-change-this-in-production
JWT_SECRET_KEY=your-jwt-secret-key-change-this-in-production

# Database
DATABASE_URL=mysql+pymysql://user:password@localhost:3306/woora_db

# Email Configuration
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USE_TLS=True
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password
MAIL_DEFAULT_SENDER=noreply@wooraentreprises.com

# Cloudinary (Image Storage)
CLOUDINARY_URL=cloudinary://api_key:api_secret@cloud_name

# Payment Providers
FEDAPAY_SECRET_KEY=your-fedapay-secret-key
FEDAPAY_PUBLIC_KEY=your-fedapay-public-key
